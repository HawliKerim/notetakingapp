generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model notes{
  id String @unique @id @db.Text
  content String @db.Text
  title String @db.Text
  createdAt DateTime @db.Timestamp() @default(now())

}

